<script>
        const d = new Date();
        const pdf ="Garbage "+d.toLocaleString();
        function generate() {
        var doc = new jspdf.jsPDF()
        doc.setFontSize(18)
        doc.autoTable({ html: '#table',
        theme: 'striped' })
        doc.save(pdf);
    }
</script>
<script>
    function ExportToExcel(){
var table = $("#table");
$(table).table2excel({
    exclude: ".noExl",
    name: "Garbage_FullList",
    filename: "Garbage_FullList_" + $.now(),
    fileext: ".xls",
    preserveColors: true,
    sheetName: "Garbage_FullList"
});

    }
</script>
